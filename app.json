{
  "expo": {
    "name": "HandJitsu",
    "slug": "HandJitsu",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logoSplash.png",
      "resizeMode": "contain",
      
      "width": 800, 
      "height": 800,
    "foregroundImage":"./assets/images/background.png",
    "backgroundColor": "#8376E5"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/background.png",
        "backgroundColor": "#8376E5"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}